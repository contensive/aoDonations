        jQuery("#donationAmount").val(5.00);
        jQuery("#donationAmount").prop("disabled", true);
    });
    //
    jQuery("#js-donAmt02").click(function() {
        jQuery("#donationAmount").val(10.00);
        jQuery("#donationAmount").prop("disabled", true);
    });
    //
    jQuery("#js-donAmt03").click(function() {
        jQuery("#donationAmount").val(15.00);
        jQuery("#donationAmount").prop("disabled", true);
    });
    //
    jQuery("#js-donAmtOther").click(function() {
        jQuery("#donationAmount").val("");
        jQuery("#donationAmount").prop("disabled", false);
    });
    jQuery("#subDonInput").click(function(e) {
        e.preventDefault();
        e.stopPropagation();
       // alert("Ready")
        var DFFirstName = jQuery("[name='DFFirstName']").val();
        var DFMiddleName = jQuery("[name='DFMiddleName']").val();
        var DFLastName = jQuery("[name='DFLastName']").val();
        var DFPhone = jQuery("[name='DFPhone']").val();
        var DFEmail = jQuery("[name='DFEmail']").val();
        var DFAddress = jQuery("[name='DFAddress']").val();
        var DFCity = jQuery("[name='DFCity']").val();
        var DFState = jQuery("[name='DFState']").val();
        console.log(DFState);
        var DFZip = jQuery("[name='DFZip']").val();
        var DFAmount = jQuery("#donationAmount").val();
        var DFType = jQuery('input[name=donate-type]:checked').val();
        //console.log(DFType);		
        var DFCardName = jQuery("#js-cardName").val();
        var DFCardNumber = jQuery("#js-cardNo").val();
        var DFPaymentMethod = jQuery('input[name=pay-method]:checked').val();
        var DFcardType = jQuery("[name='DFcardType']").val();
        //console.log(DFPaymentType);
        var DFCardCVV = jQuery("#js-cardCVV").val();
        var DFcardExp = jQuery("[name='DFcardExp']").val();
        var DFcardYr = jQuery("[name='DFcardYr']").val();
        var DFChkAccount = jQuery("#js-checkAcctName").val();
        var DFChkAccountNo = jQuery("#js-checkAcctNo").val();
        var DFChkRoutNo = jQuery("#js-checkRouteNo").val();
        //var DFSubmitted = jQuery("#js-checkRouteNo").val(1);			
        var queryString = 'DFFirstName=' + DFFirstName +
            '&DFMiddleName=' + DFMiddleName + '&DFSubmitted=1' +
            '&DFLastName=' + DFLastName + '&DFPhone=' + DFPhone +
            '&DFEmail=' + DFEmail + '&DFAddress=' + DFAddress +
            '&DFCity=' + DFCity + '&DFState=' + DFState +
            '&DFZip=' + DFZip + '&DFAmount=' + DFAmount +
            '&DFType =' + DFType + '&DFCardName=' + DFCardName +
            '&DFCardNumber=' + DFCardNumber + '&DFcardType=' +
            DFcardType + '&DFPaymentMethod=' + DFPaymentMethod +
            '&DFCardCVV=' + DFCardCVV + '&DFcardExp=' +
            DFcardExp + '&DFcardYr=' + DFcardYr +
            '&DFChkAccount=' + DFChkAccount +
            '&DFChkAccountNo=' + DFChkAccountNo +
            '&DFChkRoutNo=' + DFChkRoutNo;
        //console.log(queryString);
        cj.remote({
            'method': 'DonationFormHandler',
            'queryString': queryString,
            'callback': donationCallback
        });
    });
    //
}); //end ready
//donationCallback
//*****************************************************
function donationCallback(response) {
    //console.log(response);
    var json = (response);
    //console.log(json)
    var myObject = JSON.parse(response);
	console.log( myObject);
    if (myObject) {
        if (myObject.ProcessedOk) {
           // console.log("Process: " + myObject.ProcessedOk);
            jQuery("#donationInputForm").hide();
            jQuery("#js-donationConfirmation").show();			
            if (myObject.name) {
                // console.log("Name: " + myObject.name )
                jQuery('#tyDonater').html(myObject.name);
            }
            if (myObject.completedDate) {
                //console.log("Completed Date: " + myObject.completedDate)
                jQuery('#tyDate').html(myObject.completedDate);
            }
            if (myObject.donationAmount) {
                //console.log("donationAmount: " + myObject.donationAmount)
                jQuery('#tyAmount').html(myObject.donationAmount);
            }
            if (myObject.paymentHolderName) {
                //console.log("CreditCardLastFour: " + myObject.paymentHolderName)
                jQuery('#tyPaymentInfo').html(myObject.paymentHolderName);
            }  
			if (myObject.errorList)	{
			jquery('#js-errorList').html(myObject.errorList).show;
			}		
          if (myObject.myDFPaymentMethod == '1'){
                jQuery('#tyPayMethod').html("Check")
            }else{
                jQuery('#tyPayMethod').html("Credit Card")
             };
			 	console.log("response=" + myObject);
	
			switch(myObject.donationType) {
				case 1:
					jQuery('#tyDonateType').html("Once");
					break;
				case 2:
					jQuery('#tyDonateType').html("Monthly");
					break;
				case 3:
					jQuery('#tyDonateType').html("Quarterly");
					break;
				case 4:
					jQuery('#tyDonateType').html("Annually");
					break;
				default:
					jQuery('#tyDonateType').html("Once");
			}

			/* **************** */
        }
        /*  
     if( response=='ok') {

   }
   alert(json.repName)
   //location.reload();
  } else {

  }
*/
    }
}]]></JavascriptInHead>
